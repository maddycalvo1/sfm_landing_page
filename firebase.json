{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "./",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/about/",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/about/leadership",
        "destination": "/",
        "type": 301
      }
    ]
  }
}
